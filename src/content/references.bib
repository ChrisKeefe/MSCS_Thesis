	year = {2006},
	doi = {10.1007/11890850_16},
	note = {Series Title: Lecture Notes in Computer Science},
	pages = {148--161},
	file = {Zhao et al. - 2006 - Applying the Virtual Data Provenance Model.pdf:/home/chris/Zotero/storage/VG2VEPIV/Zhao et al. - 2006 - Applying the Virtual Data Provenance Model.pdf:application/pdf},
